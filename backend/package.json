{
  "name": "dlpz-backend",
  "version": "1.0.0",
  "description": "Backend pour dlpz.fr - Raccourcisseur d'URL et upload de fichiers",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node scripts/start-secure.js && node server.js",
    "start:unsafe": "node server.js",
    "dev": "nodemon server.js",
    "security-check": "node scripts/start-secure.js",
    "test": "node test-security.js",
    "test:security": "node test-security.js"
  },
  "keywords": [
    "url-shortener",
    "file-upload",
    "express",
    "nodejs"
  ],
  "author": "Dorian Lopez",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "nanoid": "^5.0.4",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
